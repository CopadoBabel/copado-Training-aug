    <validationRules>
        <fullName>copado__Only_One_Parent_Is_Allowed</fullName>
        <active>true</active>
        <errorConditionFormula>OR(
AND(
NOT(ISBLANK(copado__Environment__c)),
NOT(ISBLANK(copado__Pipeline__c))
),
AND(
NOT(ISBLANK(copado__Environment__c)),
NOT(ISBLANK(copado__User__c))
),
AND(
NOT(ISBLANK(copado__User__c)),
NOT(ISBLANK(copado__Pipeline__c))
)
)</errorConditionFormula>
        <errorMessage>A property can only be related to one parent: Environment, Pipeline, or User.</errorMessage>
    </validationRules>
